@model IEnumerable<DoAnASP.Areas.Admin.Models.ChiTietDatVeModel>


@{
    ViewData["Title"] = "Details";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}
<h1>Details</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TenGhe)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.datVe.khachHang.HoTen)

            </th>
            <th>
                @Html.DisplayNameFor(model => model.datVe.lichChieu.NgayChieu)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenGhe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.datVe.khachHang.HoTen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.datVe.lichChieu.NgayChieu)
            </td>
            <td>
          
                <a asp-action="Delete" asp-controller="ChiTietDatVeModels" asp-route-id="@item.IdChiTietDatVe">Delete</a>
            </td>
        </tr>
        }
    </tbody>
</table>